{
  "version": "1.8.15-*",
  "authors": [
    "Stacks Contributors"
  ],
  "description": "A flexible .NET microservice framework for agile teams.",
  "packOptions": {
    "tags": [
      "slalom",
      "stacks"
    ],
    "licenseUrl": "https://github.com/slalom-saa/stacks/blob/master/LICENSE",
    "requireLicenseAcceptance": true,
    "summary": "The core library for Stacks.",
    "projectUrl": "https://github.com/slalom-saa/stacks"
  },
  
  "dependencies": {
    "Autofac": "4.0.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../assets/stacks.ruleset",
      "/additionalfile:../../assets/stylecop.json"
    ],
    "xmlDoc": true
  },
  "frameworks": {
    "net461": {
      "dependencies": {
        "Microsoft.Extensions.Configuration": "1.1.1",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.1.1",
        "Microsoft.Extensions.Configuration.Json": "1.1.1",
        "Microsoft.Extensions.Configuration.Abstractions": "1.1.1",
        "Microsoft.Extensions.Configuration.Binder": "1.1.1",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.1"
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      },
      "imports": "dnxcore50"
    },
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Net.NameResolution": "4.3.0",
        "Microsoft.Extensions.DependencyModel": "1.1.1",
        "System.Linq.Queryable": "4.3.0",
        "System.Diagnostics.Process": "4.3.0",
        "System.Net.NetworkInformation": "4.1.0",
        "System.Security.Claims": "4.3.0",
        "System.Security.Principal.Windows": "4.3.0",
        "Microsoft.Extensions.Configuration": "1.1.1",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.1.1",
        "Microsoft.Extensions.Configuration.Json": "1.1.1",
        "System.Xml.XPath.XDocument": "4.3.0",
        "Microsoft.Extensions.Configuration.Binder": "1.1.1",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.1"
      },
      "buildOptions": {
        "define": [
          "core"
        ]
      }
    },
    "netcoreapp1.0": {
      "imports": [
        "dotnet54",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Net.NameResolution": "4.3.0",
        "Microsoft.Extensions.DependencyModel": "1.1.1",
        "System.Linq.Queryable": "4.3.0",
        "System.Diagnostics.Process": "4.3.0",
        "System.Net.NetworkInformation": "4.1.0",
        "System.Security.Claims": "4.3.0",
        "System.Security.Principal.Windows": "4.3.0",
        "Microsoft.Extensions.Configuration": "1.1.1",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.1.1",
        "Microsoft.Extensions.Configuration.Json": "1.1.1",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.1",
        "System.Xml.XPath.XDocument": "4.3.0",
        "Microsoft.Extensions.Configuration.Binder": "1.1.1"
      },
      "buildOptions": {
        "define": [
          "core"
        ]
      }
    }
  }
}
